generator client {
  provider = "prisma-client-js"
}

generator pothos {
  provider = "prisma-pothos-types"
}

datasource db {
  provider = "sqlite"
  url      = "file:../db.sqlite"
}

model Task {
  id Int @id @default(autoincrement())

  //
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
